{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "jsRules": {},
  "rules": {
    "member-access": [true, "no-public"],
    "variable-name": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "interface-name": false
  },
  "rulesDirectory": []
}
