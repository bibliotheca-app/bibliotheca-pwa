{
  "name": "functions",
  "typings": "./lib/index.d.ts",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b .",
    "build:prod": "npm run build",
    "config:prod": "firebase -P prod functions:config:set fn=\"$(cat config.prod.json)\"",
    "config:test": "firebase -P test functions:config:set fn=\"$(cat config.test.json)\"",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log",
    "_prettier": "prettier *.js \"src/**/*.ts\"",
    "prettier-format": "npm run _prettier -- --write",
    "eslint": "eslint *.js \"src/**/*.ts\" --ignore-pattern !.eslintrc.js",
    "eslint-fix": "npm run eslint -- --fix",
    "format": "run-s prettier-format eslint-fix",
    "format-check": "run-p eslint",
    "deploy": "firebase deploy --only functions"
  },
  "main": "lib/index.js",
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "@slack/web-api": "^5.1.0",
    "axios": "^0.19.0",
    "firebase-admin": "^8.3.0",
    "firebase-functions": "^3.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true
}
