{
  "name": "front",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "keywords": [],
  "dependencies": {
    "@material/react-linear-progress": "^0.15.0",
    "@types/jest": "^26.0.22",
    "@types/papaparse": "^5.2.5",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-paginate": "^6.2.1",
    "@types/styled-components": "^5.1.9",
    "@types/webpack-env": "^1.16.0",
    "firebase-tools": "^9.8.0",
    "grommet": "^2.17.1",
    "grommet-icons": "^4.5.0",
    "navi": "^0.14.0",
    "papaparse": "^5.3.0",
    "prettier": "^2.2.1",
    "react": "17.0.2",
    "react-content-loader": "^6.0.2",
    "react-dom": "17.0.2",
    "react-navi": "^0.14.4",
    "react-scripts": "4.0.3",
    "rxjs": "^6.6.6",
    "styled-components": "^5.2.1",
    "typeless": "^1.7.0"
  },
  "scripts": {
    "_prettier": "prettier \"src/**/*.{ts,tsx}\" *.js",
    "prettier-format": "yarn _prettier --write",
    "prettier-check": "yarn _prettier --list-different",
    "lint": "eslint --ext ts,tsx src",
    "lint-fix": "yarn lint --fix",
    "format": "yarn prettier-format && yarn lint-fix",
    "format-check": "yarn prettier-check && yarn lint",
    "start": "craco start",
    "build": "REACT_APP_BRANCH=$(git rev-parse --abbrev-ref HEAD) REACT_APP_VERSION=$(git rev-parse --short=10 HEAD) craco build",
    "build:prod": "REACT_APP_TARGET=prod yarn run build",
    "test": "craco test",
    "deploy": "firebase deploy --only hosting:bibliotheca-site",
    "g": "plop feature"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-useless-constructor": "off",
      "no-use-before-define": "off"
    }
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "date-fns": "^2.19.0",
    "firebase": "^8.3.1",
    "plop": "^2.7.4",
    "quagga": "^0.12.1",
    "react-feather": "^2.0.9",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
