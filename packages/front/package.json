  "keywords": [],
  "dependencies": {
    "@material/react-linear-progress": "^0.15.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.17",
    "@types/papaparse": "^5.0.1",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@types/react-paginate": "^6.2.1",
    "@types/styled-components": "^4.1.18",
    "@types/webpack-env": "^1.14.0",
    "firebase-tools": "^7.2.2",
    "grommet": "^2.7.5",
    "grommet-icons": "^4.3.0",
    "history": "^4.9.0",
    "navi": "^0.13.3",
    "papaparse": "^5.0.1",
    "prettier": "^1.18.2",
    "react": "16.9.0",
    "react-app-rewired": "2.1.3",
    "react-content-loader": "^4.2.2",
    "react-dom": "16.9.0",
    "react-navi": "^0.13.5",
    "react-scripts": "3.1.0",
    "redux": "4.0.4",
    "redux-logger": "^3.0.6",
    "remeda": "^0.0.11",
    "rxjs": "^6.5.2",
    "styled-components": "^4.3.2",
    "typeless": "^1.2.0"
  },
  "scripts": {
    "_prettier": "prettier \"src/**/*.{ts,tsx}\"",
    "prettier-format": "yarn _prettier --write",
    "prettier-check": "yarn _prettier --list-different",
    "lint": "eslint --ext ts,tsx src",
    "lint-fix": "yarn lint --fix",
    "format": "yarn prettier-format && yarn lint-fix",
    "format-check": "yarn prettier-check && yarn lint",
    "start": "react-app-rewired start",
    "build": "REACT_APP_BRANCH=$(git rev-parse --abbrev-ref HEAD) REACT_APP_VERSION=$(git rev-parse --short=10 HEAD) react-app-rewired build",
    "build:prod": "REACT_APP_TARGET=prod yarn run build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "deploy": "firebase deploy --only hosting:bibliotheca-site",
    "g": "plop feature"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-useless-constructor": "off",
      "no-use-before-define": "off"
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "babel-plugin-require-context-hook": "^1.0.0",
    "date-fns": "^1.30.1",
    "firebase": "^6.3.5",
    "plop": "^2.4.0",
    "quagga": "^0.12.1",
    "react-feather": "^2.0.3",
    "typescript": "^3.5.3"
  }
}
