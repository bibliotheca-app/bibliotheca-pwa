{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "module": "esnext",
    "lib": ["dom", "es2017"],
    "jsx": "preserve",
    "noEmit": true,
    "strictFunctionTypes": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": false,
    "skipLibCheck": true,
    "types": ["webpack-env", "jest"],
    "isolatedModules": true
  },
  "include": ["src"]
}
